<h2>Stock still remaining supplied by <%=@supplier.name%></h2>
<% stock = @supplier.stock %>
<tr>
  <% for stock_item in stock %>
  <td>
    <p><a href="/stock/<%=stock_item.id%>"><%=stock_item.distillery.name%> <%=stock_item.name%>: <%=stock_item.quantity%> bottles.</a></p>
  </td>
    <% end %>
</tr>
